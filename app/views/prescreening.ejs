		<script>
			 $(document).ready(function(){
			        $("input[type='button']").click(function(){
			        	var ans1 = $("input[name='ques1']:checked").val();
			        	var ans2 = $("input[name='ques2']:checked").val();
			        	var ans3 = $("input[name='ques3']:checked").val();
			        	var ans4 = $("input[name='ques4']:checked").val();
			        	var ans5 = $("input[name='ques5']:checked").val();
			        	var ans6 = $("input[name='ques6']:checked").val();
			        	var ans7 = $("input[name='ques7']:checked").val();

			        	let answers = [ans1, ans2, ans3, ans4, ans5, ans6, ans7]
			        	var i; 
			        	var flag = 0; 
			        	var no = 0;
			        	for(i=0; i<answers.length;i++){
			        		if(answers[i] == "yes"){
			        			flag++;
			        		}
			        		else{

			        			no++;
			        		}
			        	}

			            if(flag > no){ 
			            	var xPName = document.getElementById("PName");
			                var x = document.getElementById("positiveresult");
			                if (x.style.display === "none") {
							    x.style.display = "block";
							  } else {
							    x.style.display = "none";
							  }      
			            }
			            else{
			            	var xPName = document.getElementById("PName");
			                var x = document.getElementById("negativeresult");
			                if (x.style.display === "none") {
							    x.style.display = "block";
							  } else {
							    x.style.display = "none";
							  }  
			            }
			        });
			    });
