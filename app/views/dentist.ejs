		<script>
			// this overrides `contains` to make it case insenstive
				jQuery.expr[':'].contains = function(a, i, m) {
				  return jQuery(a).text().toUpperCase()
				      .indexOf(m[3].toUpperCase()) >= 0;
				};

				$('#search1').keyup(function (){
				    $('.media').removeClass('d-none');
				    var filter = $(this).val(); // get the value of the input, which we filter on
				    $('.mediacontent').find('.media .media-body h5:not(:contains("'+filter+'"))').parent().parent().addClass('d-none');
				})

				$('#btnSort1').click(function (){
				    $('.mediacontent .media').sort(function(a,b) {
				        return $(a).find(".media-title").text() > $(b).find(".media-title").text() ? 1 : -1;
				    }).appendTo(".mediacontent");
				})

				$('#btnexp').click(function (){
				    $('.mediacontent .media').sort(function(a,b) {
				        return $(a).find(".numb-exp").text() > $(b).find(".numb-exp").text() ? 1 : -1;
				    }).appendTo(".mediacontent");
				})

		</script>
